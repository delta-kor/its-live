generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

enum ArtistType {
  GROUP_FEMALE
  GROUP_MALE
  SOLO_FEMALE
  SOLO_MALE
}

model Artist {
  id   String     @id @default(auto()) @map("_id") @db.ObjectId
  uuid String     @unique @default(uuid())
  type ArtistType
  name String
}
